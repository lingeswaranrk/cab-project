<style>
     table,
    th,
    td {
        border: 0px;
        padding: 10px;
        border-collapse: collapse;
        border-radius: 5px;
        background-color:white;
    }
    .container{
        padding: 10px;
        padding-left: 9%;
    }

       
    body {
        background-image: url('https://images.pexels.com/photos/314374/pexels-photo-314374.jpeg?auto=compress&cs=tinysrgb&w=600');
        background-size: cover;
    }
</style>
   <center>
        <h1 class="text-center">DRIVER USER DETAILS</h1>
        <a href="/driver" class="btn btn-primary">New User </a>
</center>
<div class="container">
    
    <table class="table table-striped mt-4">
     
        
        <tr>
            <br><br>
            <th>Id</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>PASSWORD</th>
            <th>DOB</th>
            <th>ADDRESS</th>
            <th>LICENSE_NUMBER</th>
            <th>EMAIL</th>
           
        </tr>
        
        {{#each data}}
        <tr>
            <td>{{this.dataValues.id}}</td>
            <td>{{this.dataValues.firstName}}</td>
            <td>{{this.dataValues.lastName}}</td>
             <td>{{this.dataValues.password}}</td>
               <td>{{this.dataValues.dob}}</td>
            <td>{{this.dataValues.address}}</td>
            <td>{{this.dataValues.license_number}}</td>
            <td>{{this.dataValues.email}}</td>
          
            <td>
                <a href="/driverupdate/{{this.dataValues.id}}">Edit</a> |
                <a href="/delete/{{this.dataValues.id}}" class="text-danger" onclick="return confirm('Are you sure you want to delete this')">Delete</a>
            </td>
        </tr>
        {{/each}}
    </table>
  {{!--  <p class="text-end">You are logged in as: 
        <b>{{identity.firstName}} {{identity.lastName}}</b>
    </p>--}}
</div>